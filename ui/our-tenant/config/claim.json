{
    "pageBuilder": {
        "options": {
            "pageType": "claim",
            "selectors": {
                "recordEditor": ".csc-claim-template"
            },
            "components": {
                "recordEditor": {
                    "type":  "cspace.recordEditor",
                    "options": {
                        "selectors": {
                        	"identificationNumber": ".csc-claim-claimNumber"
                        },
                        "applier": "{pageBuilder}.applier",
                        "uispec": "{pageBuilder}.options.uispec.recordEditor",
                        "model": "{pageBuilder}.model"
                    }
                },
                "titleBar": {
                    "type": "cspace.titleBar",
                    "options": {
                        "fields": [
                            "fields.claimNumber",
                            "fields.claimName"
                        ]
                    }
                },
                "header": {
                    "type": "cspace.header",
                    "options": {
                        "schema": "{pageBuilder}.schema",
                        "permissions": "{pageBuilder}.permissions"
                    }
                },
                "tabs": {
                    "type": "cspace.tabs"
                },
                "sidebar": {
                    "type": "cspace.sidebar"
                }
            }
        }
    },
    "pageBuilderIO": {
        "options": {
            "recordType": "claim",
             "schema": [
                "recordlist",
                "recordtypes",
                "namespaces",
                "claim"
            ],
            "pageSpec": {
                "recordEditor": {
                    "href": "pages/ClaimTemplate.html",
                    "templateSelector": ".csc-claim-template",
                    "targetSelector": ".csc-record-edit-container"
                }
            }
        }
    }
}
