{
    "pageBuilder": {
        "options": {
            "selectors": {
                "search": ".main-search-page"
            },
            "components": {
                "search": {
                    "type": "cspace.search.searchView",
                    "options": {
                        "pivoting": true,
                        "components": {
                            "resultsPager": {
                                "options": {
                                    "model": {
                                        "pageSize": 50
                                    }
                                }
                            }
                        }
                    }
                },
                "header": {
                    "type": "cspace.header",
                    "options": {
                        "schema": "{pageBuilder}.schema",
                        "permissions": "{pageBuilder}.permissions"
                    }
                }
            }
        }
    }
}